<?xml version="1.0" encoding="utf-8"?>
<RelativeLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:id="@+id/content_pricing"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:paddingBottom="@dimen/activity_vertical_margin"

    android:paddingTop="@dimen/activity_vertical_margin"
    app:layout_behavior="@string/appbar_scrolling_view_behavior"
    tools:context="sv.devla.genesisapp.NewItems.DiscountsActivity"
    tools:showIn="@layout/activity_discounts">

    <ImageView
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        app:srcCompat="@drawable/bk_gen"
        android:id="@+id/imageView5"
        android:alpha=".5"

        />

    <TextView
        android:text="Descuentos"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"

        android:id="@+id/textView16"
        android:gravity="center"

        android:layout_marginTop="50dip"
        android:textSize="30dip"
        android:background="#90ffffff"
        android:textColor="#194B7E"

        android:layout_alignParentTop="true"
        android:layout_alignParentStart="true"
        android:layout_alignParentEnd="true" />



    <View
        android:layout_width="wrap_content"
        android:background="@drawable/grd_gen_view"
        android:layout_height="1dp"
        android:id="@+id/vvvs"
        android:layout_below="@+id/textView16"
        android:layout_marginBottom="10dip"
        android:layout_alignParentEnd="true">

    </View>

    <TextView
        android:text="."
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_alignParentTop="true"
        android:layout_centerHorizontal="true"
        android:id="@+id/textView32" />

    <EditText
        android:id="@+id/ItemCaption0"
        android:layout_height="wrap_content"
        android:layout_width="fill_parent"
        android:layout_marginLeft="10dip"
        android:padding="10dip"
        android:background="@drawable/ic_edittextbk"
        android:layout_marginRight="30dip"
        android:typeface="monospace"
        android:layout_marginTop="20dip"
        android:inputType="numberDecimal"
        android:hint="$0.00"
        android:gravity="center"
        android:layout_below="@+id/vvvs"
        android:layout_alignStart="@+id/textView32">
    </EditText>

    <TextView
        android:text="APP Minoristas"

        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:id="@+id/textView7"
        android:gravity="center"
        android:layout_marginTop="140dip"
        android:textSize="20dp"


        android:layout_marginStart="12dp"
        android:layout_alignBaseline="@+id/ItemCaption0"
        android:layout_alignBottom="@+id/ItemCaption0"
        android:layout_alignParentStart="true"
        android:layout_toStartOf="@+id/ItemCaption0" />

    <TextView
        android:text="APP Mayoristas"

        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:id="@+id/textView33"
        android:gravity="center"
        android:layout_marginTop="22dp"
        android:textSize="20dp"


        android:layout_below="@+id/ItemCaption0"
        android:layout_alignStart="@+id/textView7"
        android:layout_alignEnd="@+id/textView32" />

    <EditText
        android:id="@+id/editText3"
        android:layout_height="wrap_content"
        android:layout_width="fill_parent"
        android:layout_marginLeft="10dip"
        android:padding="10dip"
        android:background="@drawable/ic_edittextbk"
        android:layout_marginRight="30dip"
        android:typeface="monospace"
        android:inputType="numberDecimal"
        android:hint="$0.00"
        android:gravity="center"
        android:layout_alignBaseline="@+id/textView33"
        android:layout_alignBottom="@+id/textView33"
        android:layout_toEndOf="@+id/textView7"></EditText>

    <TextView
        android:text="Web"

        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:id="@+id/textView34"
        android:gravity="center"

        android:textSize="20dp"
        android:layout_marginTop="22dp"

        android:layout_below="@+id/editText3"
        android:layout_alignStart="@+id/textView33"
        android:layout_alignEnd="@+id/textView33" />

    <EditText
        android:id="@+id/editText4"
        android:layout_height="wrap_content"
        android:layout_width="fill_parent"
        android:layout_marginLeft="10dip"
        android:padding="10dip"
        android:background="@drawable/ic_edittextbk"
        android:layout_marginRight="30dip"
        android:typeface="monospace"
        android:inputType="numberDecimal"
        android:hint="$0.00"
        android:gravity="center"
        android:layout_alignBaseline="@+id/textView34"
        android:layout_alignBottom="@+id/textView34"
        android:layout_alignParentEnd="true"
        android:layout_toEndOf="@+id/textView34"></EditText>


    <TextView
        android:text="Sucursal"

        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:id="@+id/textView34s"
        android:gravity="center"

        android:textSize="20dp"
        android:layout_marginTop="20dp"

        android:layout_below="@+id/editText4"
        android:layout_alignStart="@+id/textView34"
        android:layout_alignEnd="@+id/textView34" />


    <EditText
        android:id="@+id/editTextsucursal"
        android:layout_height="wrap_content"
        android:layout_width="fill_parent"
        android:layout_marginLeft="10dip"
        android:padding="10dip"
        android:background="@drawable/ic_edittextbk"
        android:layout_marginRight="30dip"
        android:typeface="monospace"
        android:inputType="numberDecimal"
        android:hint="$0.00"
        android:gravity="center"
        android:layout_alignBaseline="@+id/textView34s"
        android:layout_alignBottom="@+id/textView34s"
        android:layout_toEndOf="@+id/textView7"></EditText>


</RelativeLayout>
