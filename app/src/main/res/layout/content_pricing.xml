<?xml version="1.0" encoding="utf-8"?>
<RelativeLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:id="@+id/content_pricing"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:paddingBottom="@dimen/activity_vertical_margin"

    android:paddingTop="@dimen/activity_vertical_margin"
    app:layout_behavior="@string/appbar_scrolling_view_behavior"
    tools:context="sv.devla.genesisapp.NewItems.PricingActivity"
    tools:showIn="@layout/activity_pricing">

    <ImageView
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        app:srcCompat="@drawable/bk_gen"
        android:id="@+id/imageView5"
        android:alpha=".5"

        />

    <TextView
        android:text="Precio de referencia"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"

        android:id="@+id/txtrefer"
        android:gravity="center"

        android:textSize="20dip"
        android:background="#90ffffff"
        android:textColor="#194B7E"

        android:layout_below="@+id/txtpreciosucursal"
        android:layout_alignParentStart="true"
        android:layout_marginTop="29dp"
        android:layout_alignParentEnd="true" />

    <TextView
        android:text="Precios"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"

        android:id="@+id/textView16"
        android:gravity="center"

        android:layout_marginTop="50dip"
        android:textSize="30dip"
        android:background="#90ffffff"
        android:textColor="#194B7E"

        android:layout_alignParentTop="true"
        android:layout_alignParentStart="true"
        android:layout_alignParentEnd="true" />



    <View
        android:layout_width="wrap_content"
        android:background="@drawable/grd_gen_view"
        android:layout_height="1dp"
        android:id="@+id/vvvs"
        android:layout_below="@+id/textView16"
        android:layout_marginBottom="10dip"
        android:layout_alignParentEnd="true">

    </View>

    <TextView
        android:text="."
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_alignParentTop="true"
        android:layout_centerHorizontal="true"
        android:id="@+id/textView32" />

    <EditText
        android:id="@+id/txtpreciominorista"
        android:layout_height="wrap_content"
        android:layout_width="fill_parent"
        android:layout_marginLeft="10dip"
        android:padding="10dip"
        android:background="@drawable/ic_edittextbk"
        android:layout_marginRight="30dip"
        android:typeface="monospace"
        android:layout_marginTop="20dip"
        android:inputType="numberDecimal"
        android:hint="$0.00"
        android:gravity="center"
        android:layout_below="@+id/vvvs"
        android:layout_alignStart="@+id/textView32">
    </EditText>

    <TextView
        android:text="APP Minoristas"

        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:id="@+id/ttmin"
        android:gravity="center"
        android:layout_marginTop="140dip"
        android:textSize="20dp"


        android:layout_marginStart="12dp"
        android:layout_alignBaseline="@+id/txtpreciominorista"
        android:layout_alignBottom="@+id/txtpreciominorista"
        android:layout_alignParentStart="true"
        android:layout_toStartOf="@+id/txtpreciominorista" />

    <TextView
        android:text="APP Mayoristas"

        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:id="@+id/ttmax"
        android:gravity="center"
        android:layout_marginTop="22dp"
        android:textSize="20dp"


        android:layout_below="@+id/txtpreciominorista"
        android:layout_alignStart="@+id/ttmin"
        android:layout_alignEnd="@+id/textView32" />

    <EditText
        android:id="@+id/txtpreciomayorista"
        android:layout_height="wrap_content"
        android:layout_width="fill_parent"
        android:layout_marginLeft="10dip"
        android:padding="10dip"
        android:background="@drawable/ic_edittextbk"
        android:layout_marginRight="30dip"
        android:typeface="monospace"
        android:inputType="numberDecimal"
        android:hint="$0.00"
        android:gravity="center"
        android:layout_alignBaseline="@+id/ttmax"
        android:layout_alignBottom="@+id/ttmax"
        android:layout_toEndOf="@+id/ttmin"></EditText>

    <TextView
        android:text="Web"

        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:id="@+id/ttweb"
        android:gravity="center"

        android:textSize="20dp"
        android:layout_marginTop="22dp"

        android:layout_below="@+id/txtpreciomayorista"
        android:layout_alignStart="@+id/ttmax"
        android:layout_alignEnd="@+id/ttmax" />

    <EditText
        android:id="@+id/txtprecioweb"
        android:layout_height="wrap_content"
        android:layout_width="fill_parent"
        android:layout_marginLeft="10dip"
        android:padding="10dip"
        android:background="@drawable/ic_edittextbk"
        android:layout_marginRight="30dip"
        android:typeface="monospace"
        android:inputType="numberDecimal"
        android:hint="$0.00"
        android:gravity="center"
        android:layout_alignBaseline="@+id/ttweb"
        android:layout_alignBottom="@+id/ttweb"
        android:layout_alignParentEnd="true"
        android:layout_toEndOf="@+id/ttweb"></EditText>


    <TextView
        android:text="Sucursal"

        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:id="@+id/ttsuc"
        android:gravity="center"

        android:textSize="20dp"
        android:layout_marginTop="20dp"

        android:layout_below="@+id/txtprecioweb"
        android:layout_alignStart="@+id/ttweb"
        android:layout_alignEnd="@+id/ttweb" />


    <EditText
        android:id="@+id/txtpreciosucursal"
        android:layout_height="wrap_content"
        android:layout_width="fill_parent"
        android:layout_marginLeft="10dip"
        android:padding="10dip"
        android:background="@drawable/ic_edittextbk"
        android:layout_marginRight="30dip"
        android:typeface="monospace"
        android:inputType="numberDecimal"
        android:hint="$0.00"
        android:gravity="center"
        android:layout_alignBaseline="@+id/ttsuc"
        android:layout_alignBottom="@+id/ttsuc"
        android:layout_toEndOf="@+id/ttmin"></EditText>

     <TextView
        android:text="$10.99 - $11.99 "
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"

        android:id="@+id/txtpreciosdereferencia"
        android:gravity="center"

        android:textSize="20dip"
        android:background="#90ffffff"
        android:textColor="#194B7E"

        android:layout_below="@+id/txtrefer"
        android:layout_alignParentStart="true"
        android:layout_alignParentEnd="true" />


</RelativeLayout>
